{
  "manifest_version": 2,

  "name": "Chrome Navigation",
  "description": "An extension to browse google chrome handsfree",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "speechtotext.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [ "background.js" ]
    }
      ],
  "background": {
  "scripts": ["background.js"],
  "persistent": false
},
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "chrome-extension://pepfekbcnonajjmpnhoejlegjohkcbld/background.js:89:25",
    "<all_urls>"
  ]
}
